{
  "jsc": {
    "baseUrl": ".",
    "paths": {},
    "externalHelpers": true,
    "target": "es2020",
    "parser": {
      "syntax": "typescript",
      "decorators": true,
      "dynamicImport": true
    },
    "transform": {
      "legacyDecorator": true,
      "decoratorMetadata": true
    },
    "keepClassNames": true
  },
  "module": {
    "strict": true,
    "type": "commonjs"
  }
}
