    "@swc/core": "^1.2.85",
    "@swc/helpers": "^0.2.13",
    "@typechain/ethers-v5": "^7.0.1",
    "@typechain/hardhat": "^2.3.0",
    "@types/chai": "^4.2.21",
    "@types/fs-extra": "^9.0.12",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.0",
    "chai": "^4.3.4",
    "chalk": "^4.1.2",
    "dotenv": "^10.0.0",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.4.6",
    "fs-extra": "^10.0.0",
    "hardhat": "^2.6.3",
    "hardhat-deploy": "^0.9.1",
    "hardhat-gas-reporter": "^1.0.4",
    "hardhat-packager": "^1.1.0",
    "mocha": "^9.1.1",
    "prettier": "^2.3.2",
    "prettier-plugin-solidity": "^1.0.0-beta.17",
    "solhint": "^3.3.6",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.7.17",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typechain": "^5.1.2",
    "typescript": "~4.4.2"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": "please-use-yarn",
    "yarn": ">=1.22.5"
  },
  "keywords": [
    "blockchain",
    "ethereum",
    "hardhat",
    "smart-contracts",
    "solidity"
  ],
  "license": "ISC",
  "private": true,
  "repository": "github:gulitsky/solidity-starter",
  "scripts": {
    "build": "hardhat compile",
    "clean": "hardhat clean",
    "coverage": "hardhat coverage",
    "deploy": "hardhat deploy",
    "lint": "yarn run lint:sol && yarn prettier:check",
    "lint:sol": "solhint --config ./.solhint.json --max-warning 0 \"./contracts/**/*.sol\"",
    "prettier": "prettier --config ./.prettierrc --write \"**/*.{js,json,md,sol,ts}\"",
    "prettier:check": "prettier --check --config ./.prettierrc --write \"**/*.{js,json,md,sol,ts}\"",
    "rebuild": "yarn run build --force",
    "test": "hardhat test",
    "typechain": "hardhat typechain"
  }
}
