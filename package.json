{
  "name": "@gulitsky/solidity-starter",
  "bugs": "https://github.com/gulitsky/solidity-starter",
  "version": "0.1.0-canary",
  "description": "",
  "devDependencies": {
    "@codechecks/client": "^0.1.11",
    "@commitlint/cli": "^13.2.0",
    "@commitlint/config-conventional": "^13.2.0",
    "@commitlint/cz-commitlint": "^13.2.0",
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers@^0.3.0-beta.10",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@swc/core": "^1.2.92",
    "@swc/helpers": "^0.2.13",
    "@swc/register": "^0.1.7",
    "@typechain/ethers-v5": "^7.1.2",
    "@typechain/hardhat": "^2.3.0",
    "@types/chai": "^4.2.22",
    "@types/fs-extra": "^9.0.13",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.10.1",
    "chai": "^4.3.4",
    "chalk": "^4.1.2",
    "commitizen": "^4.2.4",
    "dotenv": "^10.0.0",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.4.7",
    "fs-extra": "^10.0.0",
    "hardhat": "^2.6.4",
    "hardhat-deploy": "^0.9.2",
    "hardhat-gas-reporter": "^1.0.4",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "mocha": "^9.1.2",
    "prettier": "^2.4.1",
    "prettier-plugin-solidity": "^1.0.0-beta.18",
    "semantic-release": "^18.0.0",
    "solhint": "^3.3.6",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.7.17",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typechain": "^5.1.2",
    "typescript": "~4.4.3"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": "please-use-yarn",
    "yarn": ">=1.22.5"
  },
  "keywords": [
    "blockchain",
    "ethereum",
    "hardhat",
    "smart-contracts",
    "solidity"
  ],
  "license": "ISC",
  "private": true,
  "repository": "github:gulitsky/solidity-starter",
  "scripts": {
    "build": "hardhat compile",
    "clean": "hardhat clean",
    "commit": "cz",
    "coverage": "hardhat coverage --solcoverjs ./.solcover.js --temp ./artifacts/ --testfiles \"./test/**/*.ts\"",
    "deploy": "hardhat deploy",
    "lint:sol": "solhint --config ./.solhint.json --max-warnings 0 \"./contracts/**/*.sol\"",
    "lint": "yarn run lint:sol && yarn prettier",
    "prepare": "husky install",
    "prettier": "prettier --check --config ./.prettierrc  \"./**/*.{js,json,md,sol,ts,yml}\"",
    "prettier:fix": "prettier --config ./.prettierrc --write \"./**/*.{js,json,md,sol,ts,yml}\"",
    "rebuild": "yarn run build --force",
    "test": "hardhat test",
    "typechain": "hardhat typechain",
    "verify": "hardhat etherscan-verify --solc-input"
  },
  "packageManager": "yarn@3.0.2"
}
